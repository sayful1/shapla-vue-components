{
  "name": "shapla-media-uploader",
  "description": "A collection of vue component for handling frontend upload functionality",
  "version": "1.0.0",
  "author": {
    "name": "Sayful Islam",
    "email": "sayful.islam001@gmail.com",
    "url": "https://sayfulislam.com"
  },
  "keywords": [
    "vue",
    "vuejs",
    "plugin",
    "grid",
    "column"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sayful1/shapla-vue-components.git"
  },
  "bugs": {
    "url": "https://github.com/sayful1/shapla-vue-components/issues"
  },
  "main": "dist/media-uploader.common.js",
  "unpkg": "dist/media-uploader.umd.js",
  "module": "src/index.js",
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:lib": "vue-cli-service build --target lib --name media-uploader ./src/index.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "dropzone": "^5.5.1",
    "shapla-button": "^1.0.5",
    "shapla-color-system": "^1.0.2",
    "shapla-columns": "^1.0.3",
    "shapla-delete": "^1.0.2",
    "shapla-modal": "^1.0.4",
    "vue": "^2.6.11",
    "vue2-dropzone": "^3.6.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.2",
    "@vue/cli-plugin-eslint": "^4.1.2",
    "@vue/cli-service": "^4.1.2",
    "autoprefixer": "^9.7.4",
    "babel-eslint": "^10.0.3",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.1.2",
    "node-sass": "^4.13.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
